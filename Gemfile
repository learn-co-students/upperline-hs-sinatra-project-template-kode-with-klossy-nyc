# A sample Gemfile
source "https://rubygems.org"

gem 'pry'
gem 'sinatra'
gem 'shotgun'
gem 'httparty'
gem 'heroku'


# 1. update gemfile with 'heroku'
# 2. bundle install
# 3. heroku login
# (heroku create <project name>)
# 4. git add .
# 5. git commit -m "message"
# 6. git push origin master
# 7. git push heroku master
